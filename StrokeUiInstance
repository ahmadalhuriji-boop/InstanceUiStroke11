MainScreenGui.CloseUITextButton.MouseButton1Click:Connect(function()
	MainScreenGui.CloseGuiWarning.Visible = true
	MainScreenGui.MainFrame.Visible = false
end)

MainScreenGui.YesTextButton.MouseButton1Click:Connect(function()
	MainScreenGui.MainScreenGui:Destroy()
end)
MainScreenGui.NoTextButton.MouseButton1Click:Connect(function()
	MainScreenGui.CloseGuiWarning.Visible = false
	MainScreenGui.MainFrame.Visible = true
end)

local gui = MainScreenGui.MainFrame
local dragging = false
local dragStart, startPos

local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(
		startPos.X.Scale,
		startPos.X.Offset + delta.X,
		startPos.Y.Scale,
		startPos.Y.Offset + delta.Y
	)
end

MainScreenGui.MoveUiTextButton.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
		update(input)
	end
end)

disableInfiniteJump()
Start()
