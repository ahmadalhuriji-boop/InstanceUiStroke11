local TweenService = game:GetService("TweenService")

local function Start()
	MainScreenGui.MainScreenGui.Enabled = true
	MainScreenGui.PlayerFrame.Visible = false
	MainScreenGui.HomeFrame.Visible = true
	MainScreenGui.TeleportFrame.Visible = false
	MainScreenGui.TrollFrame.Visible = false
	MainScreenGui.ESPFrame.Visible = false
	MainScreenGui.CloseGuiWarning.Visible = false

	local tweenInfo =
		TweenInfo.new(
			1, -- seconds
			Enum.EasingStyle.Quad,
			Enum.EasingDirection.Out
		)

	-- Assuming UI.MainFrame has one UIStroke child (adjust if multiple)
	local uiStroke = MainScreenGui.MainFrame:FindFirstChildWhichIsA("UIStroke")
	if uiStroke then
		uiStroke.Transparency = 1 -- start invisible
		local strokeGoal = {Transparency = 0} -- fully visible
		local strokeTween = TweenService:Create(uiStroke, tweenInfo, strokeGoal)
		strokeTween:Play()
	end

	-- Tween UI.Notifi position as before
	local goal = {Position = UDim2.new(1.5, 0, 0.9, 0)}
	local goal2 = {Position = UDim2.new(0.9, 0, 0.9, 0)}

	wait(1)

	local tween2 = TweenService:Create(MainScreenGui.Notifi, tweenInfo, goal2)
	tween2:Play()

	wait(5)

	local tween = TweenService:Create(MainScreenGui.Notifi, tweenInfo, goal)
	tween:Play()

	tween.Completed:Connect(
		function()
			MainScreenGui.Notifi.Visible = false
		end
	)
end

local function TeleportToGun()
	-- Tween target position
	local goal = {}
	local goal2 = {}
	goal.Position = UDim2.new(1.5, 0, 0.9, 0)
	goal2.Position = UDim2.new(0.9, 0, 0.9, 0)

	-- Tween settings
	local tweenInfo =
		TweenInfo.new(
			1, -- Time in seconds
			Enum.EasingStyle.Quad,
			Enum.EasingDirection.Out
		)

	MainScreenGui.GunThings.Visible = true
	MainScreenGui.WhatHappenToTheGun.Text = "Teleporting To The Gun..."

	local tween2 = TweenService:Create(MainScreenGui.GunThings, tweenInfo, goal2)
	tween2:Play()

	-- Create and play tween
	local tween = TweenService:Create(MainScreenGui.GunThings, tweenInfo, goal)
	wait(5)
	tween:Play()

	-- Wait until tween finishes, then hide
	tween.Completed:Connect(
		function()
			MainScreenGui.GunThings.Visible = false
		end
	)
end

local function NoGunFound()
	-- Tween target position
	local goal = {}
	local goal2 = {}
	goal.Position = UDim2.new(1.5, 0, 0.9, 0)
	goal2.Position = UDim2.new(0.9, 0, 0.9, 0)

	-- Tween settings
	local tweenInfo =
		TweenInfo.new(
			1, -- Time in seconds
			Enum.EasingStyle.Quad,
			Enum.EasingDirection.Out
		)

	MainScreenGui.GunThings.Visible = true
	MainScreenGui.WhatHappenToTheGun.Text = "The Gun Did NOT Drop"

	local tween2 = TweenService:Create(MainScreenGui.GunThings, tweenInfo, goal2)
	tween2:Play()

	-- Create and play tween
	local tween = TweenService:Create(MainScreenGui.GunThings, tweenInfo, goal)
	wait(5)
	tween:Play()

	-- Wait until tween finishes, then hide
	tween.Completed:Connect(
		function()
			MainScreenGui.GunThings.Visible = false
		end
	)
end

