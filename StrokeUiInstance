local executor = "Unknown"

if solara then
	executor = "Solara"
elseif getexecutorname then
	local success, name = pcall(getexecutorname)
	if success and name then
		executor = name
	end
elseif identifyexecutor then
	local success, name = pcall(identifyexecutor)
	if success and name then
		executor = name
	end
elseif syn and syn.request then
	executor = "Synapse X"
elseif KRNL_LOADED then
	executor = "KRNL"
elseif isfluxus then
	executor = "Fluxus"
end

print("Executor Detected: " .. executor)


local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LP = Players.LocalPlayer
local roles

-- > Functions <--

function CreateHighlight() -- make any new highlights for new players
	for i, v in pairs(Players:GetChildren()) do
		if v ~= LP and v.Character and not v.Character:FindFirstChild("Highlight") then
			Instance.new("Highlight", v.Character)           
		end
	end
end

local RoleColors = {
	Murderer = Color3.fromRGB(225, 0, 0),
	Sheriff = Color3.fromRGB(0, 0, 225),
	Hero = Color3.fromRGB(255, 250, 0),
	Innocent = Color3.fromRGB(0, 225, 0)
}



function UpdateHighlights() -- Get Current Role Colors (messy)
	for _, v in pairs(Players:GetChildren()) do
		if v ~= LP and v.Character and v.Character:FindFirstChild("Highlight") then
			Highlight = v.Character:FindFirstChild("Highlight")
			if v.Name == Sheriff and IsAlive(v) then
				Highlight.FillColor = Color3.fromRGB(0, 0, 225)
			elseif v.Name == Murder and IsAlive(v) then
				Highlight.FillColor = Color3.fromRGB(225, 0, 0)
			elseif v.Name == Hero and IsAlive(v) and not IsAlive(game.Players[Sheriff]) then
				Highlight.FillColor = Color3.fromRGB(255, 250, 0)
			else
				Highlight.FillColor = Color3.fromRGB(0, 225, 0)
			end
		end
	end
end	

function IsAlive(Player) -- Simple sexy function
	for i, v in pairs(roles) do
		if Player.Name == i then
			if not v.Killed and not v.Dead then
				return true
			else
				return false
			end
		end
	end
end

local function CreateNameTag()
	for _, v in pairs(Players:GetChildren()) do
		if v ~= LP and v.Character and v.Character:FindFirstChild("Head") then
			if not v.Character:FindFirstChild("NameTag") then
				local billboard = Instance.new("BillboardGui")
				billboard.Name = "NameTag"
				billboard.Parent = v.Character
				billboard.Adornee = v.Character.Head
				billboard.Size = UDim2.new(0, 200, 0, 50)
				billboard.StudsOffset = Vector3.new(0, 2, 0)
				billboard.AlwaysOnTop = true

				local textLabel = Instance.new("TextLabel")
				textLabel.Name = "RoleName"
				textLabel.Parent = billboard
				textLabel.Size = UDim2.new(1, 0, 1, 0)
				textLabel.BackgroundTransparency = 1
				textLabel.TextScaled = false
				textLabel.TextSize = 20
				textLabel.Font = Enum.Font.SourceSansBold
				textLabel.Text = v.Name
				textLabel.TextColor3 = RoleColors.Innocent -- default color
			end
		end
	end
end

-- Update name colors
local function UpdateNameColors()
	for _, v in pairs(Players:GetChildren()) do
		if v ~= LP and v.Character then
			local nameTag = v.Character:FindFirstChild("NameTag")
			local textLabel = nameTag and nameTag:FindFirstChild("RoleName")
			if textLabel then
				if v.Name == Sheriff and IsAlive(v) then
					textLabel.TextColor3 = RoleColors.Sheriff
				elseif v.Name == Murder and IsAlive(v) then
					textLabel.TextColor3 = RoleColors.Murderer
				elseif v.Name == Hero and IsAlive(v) and not IsAlive(Players[Sheriff]) then
					textLabel.TextColor3 = RoleColors.Hero
				else
					textLabel.TextColor3 = RoleColors.Innocent
				end
			end
		end
	end
end



