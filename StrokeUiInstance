MainScreenGui.KillAllButton.MouseButton1Click:Connect(
	function()
		local targetPosition = Vector3.new(-30, -45, 18)

		for _, plr in pairs(game.Players:GetPlayers()) do
			if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				plr.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
			end
		end
	end
)



MainScreenGui.FlingMurderButton.MouseButton1Click:Connect(function()
	local Targets = {
		"All"
	} -- "All", "Target Name", "arian_was_here"
	local Players = game:GetService("Players")
	local Player = Players.LocalPlayer
	local AllBool = false

	local function GetPlayer(Name)
		Name = Name:lower()
		if Name == "all" or Name == "others" then
			AllBool = true
			return
		elseif Name == "random" then
			local GetPlayers = Players:GetPlayers()
			if table.find(GetPlayers, Player) then
				table.remove(GetPlayers, table.find(GetPlayers, Player))
			end
			return GetPlayers[math.random(#GetPlayers)]
		elseif Name ~= "random" and Name ~= "all" and Name ~= "others" then
			for _, x in next, Players:GetPlayers() do
				if x ~= Player then
					if x.Name:lower():match("^" .. Name) or x.DisplayName:lower():match("^" .. Name) then
						return x
					end
				end
			end
		end
	end

	local function Message(_Title, _Text, Time)
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = _Title,
			Text = _Text,
			Duration = Time
		})
	end

	local function SkidFling(TargetPlayer)
		local Character = Player.Character
		local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
		local RootPart = Humanoid and Humanoid.RootPart
		local TCharacter = TargetPlayer.Character
		local THumanoid, TRootPart, THead, Accessory, Handle

		if TCharacter then
			THumanoid = TCharacter:FindFirstChildOfClass("Humanoid")
			if THumanoid then TRootPart = THumanoid.RootPart end
			THead = TCharacter:FindFirstChild("Head")
			Accessory = TCharacter:FindFirstChildOfClass("Accessory")
			if Accessory and Accessory:FindFirstChild("Handle") then
				Handle = Accessory.Handle
			end
		end

		if Character and Humanoid and RootPart then
			if RootPart.Velocity.Magnitude < 50 then
				getgenv().OldPos = RootPart.CFrame
			end

			if THead then
				workspace.CurrentCamera.CameraSubject = THead
			elseif Handle then
				workspace.CurrentCamera.CameraSubject = Handle
			elseif THumanoid and TRootPart then
				workspace.CurrentCamera.CameraSubject = THumanoid
			end

			if not TCharacter:FindFirstChildWhichIsA("BasePart") then
				return
			end

			local function FPos(BasePart, Pos, Ang)
				RootPart.CFrame = CFrame.new(BasePart.Position) * Pos * Ang
				Character:SetPrimaryPartCFrame(CFrame.new(BasePart.Position) * Pos * Ang)
				RootPart.Velocity = Vector3.new(9e7, 9e7 * 10, 9e7)
				RootPart.RotVelocity = Vector3.new(9e8, 9e8, 9e8)
			end

			local function SFBasePart(BasePart)
				local TimeToWait, Time, Angle = 2, tick(), 0
				repeat
					if RootPart and THumanoid then
						if BasePart.Velocity.Magnitude < 50 then
							Angle += 100
							FPos(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0 , 0))
							task.wait()
							FPos(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
							task.wait()
						else
							FPos(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
							task.wait()
							FPos(BasePart, CFrame.new(0, -1.5, -THumanoid.WalkSpeed), CFrame.Angles(0, 0, 0))
							task.wait()
						end
					else
						break
					end
				until BasePart.Velocity.Magnitude > 500 or BasePart.Parent ~= TCharacter or TargetPlayer.Parent ~= Players or not TargetPlayer.Character == TCharacter or THumanoid.Sit or Humanoid.Health <= 0 or tick() > Time + TimeToWait
			end

			workspace.FallenPartsDestroyHeight = 0/0
			local BV = Instance.new("BodyVelocity")
			BV.Name = "EpixVel"
			BV.Parent = RootPart
			BV.Velocity = Vector3.new(9e8, 9e8, 9e8)
			BV.MaxForce = Vector3.new(1/0, 1/0, 1/0)
			Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)

			if TRootPart and THead then
				if (TRootPart.Position - THead.Position).Magnitude > 5 then
					SFBasePart(THead)
				else
					SFBasePart(TRootPart)
				end
			elseif TRootPart then
				SFBasePart(TRootPart)
			elseif THead then
				SFBasePart(THead)
			elseif Handle then
				SFBasePart(Handle)
			else
				return Message("Error Occurred", "Target is missing all parts", 5)
			end

			BV:Destroy()
			Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
			workspace.CurrentCamera.CameraSubject = Humanoid

			-- Return to original position after fling
			if getgenv().OldPos then
				repeat
					RootPart.CFrame = getgenv().OldPos * CFrame.new(0, .5, 0)
					Character:SetPrimaryPartCFrame(getgenv().OldPos * CFrame.new(0, .5, 0))
					Humanoid:ChangeState("GettingUp")
					for _, x in pairs(Character:GetChildren()) do
						if x:IsA("BasePart") then
							x.Velocity, x.RotVelocity = Vector3.new(), Vector3.new()
						end
					end
					task.wait()
				until (RootPart.Position - getgenv().OldPos.Position).Magnitude < 25
			end

			-- Clear OldPos so it won't teleport later
			getgenv().OldPos = nil
			workspace.FallenPartsDestroyHeight = getgenv().FPDH
		end
	end

	getgenv().Welcome = true

	if Targets[1] then
		for _, x in next, Targets do
			GetPlayer(x)
		end
	else
		return
	end

	if AllBool then
		for _, x in pairs(Players:GetPlayers()) do
			if x.Backpack:FindFirstChild("Knife") or (x.Character and x.Character:FindFirstChild("Knife")) then
				SkidFling(x)
			end
		end
	end
end)


MainScreenGui.FlingSheriffButton.MouseButton1Click:Connect(function()
	local Targets = {
		"All"
	} -- "All", "Target Name", "arian_was_here"
	local Players = game:GetService("Players")
	local Player = Players.LocalPlayer
	local AllBool = false

	local function GetPlayer(Name)
		Name = Name:lower()
		if Name == "all" or Name == "others" then
			AllBool = true
			return
		elseif Name == "random" then
			local GetPlayers = Players:GetPlayers()
			if table.find(GetPlayers, Player) then
				table.remove(GetPlayers, table.find(GetPlayers, Player))
			end
			return GetPlayers[math.random(#GetPlayers)]
		elseif Name ~= "random" and Name ~= "all" and Name ~= "others" then
			for _, x in next, Players:GetPlayers() do
				if x ~= Player then
					if x.Name:lower():match("^" .. Name) or x.DisplayName:lower():match("^" .. Name) then
						return x
					end
				end
			end
		end
	end

	local function Message(_Title, _Text, Time)
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = _Title,
			Text = _Text,
			Duration = Time
		})
	end

	local function SkidFling(TargetPlayer)
		local Character = Player.Character
		local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
		local RootPart = Humanoid and Humanoid.RootPart
		local TCharacter = TargetPlayer.Character
		local THumanoid, TRootPart, THead, Accessory, Handle

		if TCharacter then
			THumanoid = TCharacter:FindFirstChildOfClass("Humanoid")
			if THumanoid then TRootPart = THumanoid.RootPart end
			THead = TCharacter:FindFirstChild("Head")
			Accessory = TCharacter:FindFirstChildOfClass("Accessory")
			if Accessory and Accessory:FindFirstChild("Handle") then
				Handle = Accessory.Handle
			end
		end

		if Character and Humanoid and RootPart then
			if RootPart.Velocity.Magnitude < 50 then
				getgenv().OldPos = RootPart.CFrame
			end

			if THead then
				workspace.CurrentCamera.CameraSubject = THead
			elseif Handle then
				workspace.CurrentCamera.CameraSubject = Handle
			elseif THumanoid and TRootPart then
				workspace.CurrentCamera.CameraSubject = THumanoid
			end

			if not TCharacter:FindFirstChildWhichIsA("BasePart") then
				return
			end

			local function FPos(BasePart, Pos, Ang)
				RootPart.CFrame = CFrame.new(BasePart.Position) * Pos * Ang
				Character:SetPrimaryPartCFrame(CFrame.new(BasePart.Position) * Pos * Ang)
				RootPart.Velocity = Vector3.new(9e7, 9e7 * 10, 9e7)
				RootPart.RotVelocity = Vector3.new(9e8, 9e8, 9e8)
			end

			local function SFBasePart(BasePart)
				local TimeToWait, Time, Angle = 2, tick(), 0
				repeat
					if RootPart and THumanoid then
						if BasePart.Velocity.Magnitude < 50 then
							Angle += 100
							FPos(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0 , 0))
							task.wait()
							FPos(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
							task.wait()
						else
							FPos(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
							task.wait()
							FPos(BasePart, CFrame.new(0, -1.5, -THumanoid.WalkSpeed), CFrame.Angles(0, 0, 0))
							task.wait()
						end
					else
						break
					end
				until BasePart.Velocity.Magnitude > 500 or BasePart.Parent ~= TCharacter or TargetPlayer.Parent ~= Players or not TargetPlayer.Character == TCharacter or THumanoid.Sit or Humanoid.Health <= 0 or tick() > Time + TimeToWait
			end

			workspace.FallenPartsDestroyHeight = 0/0
			local BV = Instance.new("BodyVelocity")
			BV.Name = "EpixVel"
			BV.Parent = RootPart
			BV.Velocity = Vector3.new(9e8, 9e8, 9e8)
			BV.MaxForce = Vector3.new(1/0, 1/0, 1/0)
			Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)

			if TRootPart and THead then
				if (TRootPart.Position - THead.Position).Magnitude > 5 then
					SFBasePart(THead)
				else
					SFBasePart(TRootPart)
				end
			elseif TRootPart then
				SFBasePart(TRootPart)
			elseif THead then
				SFBasePart(THead)
			elseif Handle then
				SFBasePart(Handle)
			end

			BV:Destroy()
			Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
			workspace.CurrentCamera.CameraSubject = Humanoid

			-- Return to original position after fling
			if getgenv().OldPos then
				repeat
					RootPart.CFrame = getgenv().OldPos * CFrame.new(0, .5, 0)
					Character:SetPrimaryPartCFrame(getgenv().OldPos * CFrame.new(0, .5, 0))
					Humanoid:ChangeState("GettingUp")
					for _, x in pairs(Character:GetChildren()) do
						if x:IsA("BasePart") then
							x.Velocity, x.RotVelocity = Vector3.new(), Vector3.new()
						end
					end
					task.wait()
				until (RootPart.Position - getgenv().OldPos.Position).Magnitude < 25
			end

			-- Clear OldPos so it won't teleport later
			getgenv().OldPos = nil
			workspace.FallenPartsDestroyHeight = getgenv().FPDH
		else
			return Message("Error Occurred", "Random error", 5)
		end
	end
	getgenv().Welcome = true

	if Targets[1] then
		for _, x in next, Targets do
			GetPlayer(x)
		end
	else
		return
	end

	if AllBool then
		for _, x in pairs(Players:GetPlayers()) do
			if x.Backpack:FindFirstChild("Gun") or (x.Character and x.Character:FindFirstChild("Gun")) then
				SkidFling(x)
			end
		end
	end
end)

MainScreenGui.GrabGunButton.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local rootPart = character:FindFirstChild("HumanoidRootPart")

	local mm2Maps = {
		"Office2", "Office3", "Bank", "Bank2", "Hospital2", "Hospital3",
		"ResearchFacility", "Factory", "MilBase", "Hotel", "Hotel2",
		"PoliceStation", "Workplace", "Biolab", "House2", "House3",
		"Mansion", "Mansion2", "Station", "LogCabin", "Farmhouse", "Workshop"
	}

	-- Check for map in the Workspace
	local mapFolder
	for _, mapName in pairs(mm2Maps) do
		local map = workspace:FindFirstChild(mapName)
		if map then
			mapFolder = map
			break
		end
	end

	if mapFolder then
		local gunDrop = mapFolder:FindFirstChild("GunDrop", true)
		if gunDrop and gunDrop:IsA("BasePart") then
			local originalCFrame = rootPart.CFrame

			rootPart.CFrame = gunDrop.CFrame

			wait(1)

			rootPart.CFrame = originalCFrame

			GoingBackToOldPos()
		else
			NoGunFound()
		end
	else
		NoGunFoundinMap()
	end
end)


local maxWalkSpeed = 200
local maxJumpPower = 500

-- Starting values (you can set initial values here)
local currentWalkSpeed = 16
local currentJumpPower = 50

-- Update UI text initially
MainScreenGui.WalkSpeedValueText.Text = tostring(currentWalkSpeed)
MainScreenGui.JumpPowerValueText.Text = tostring(currentJumpPower)

-- Sizes for frames when at max value
local walkSpeedFullWidth = 293
local walkSpeedHeight = 28

local jumpPowerFullWidth = 293
local jumpPowerHeight = 28

-- Apply values to player
local function applyWalkSpeed(speed)
	local player = game.Players.LocalPlayer
	if player.Character and player.Character:FindFirstChild("Humanoid") then
		player.Character.Humanoid.WalkSpeed = speed
	end
end

local function applyJumpPower(power)
	local player = game.Players.LocalPlayer
	if player.Character and player.Character:FindFirstChild("Humanoid") then
		player.Character.Humanoid.JumpPower = power
	end
end

-- Functions to update the size of the bars based on values
local function updateWalkSpeedFrame(walkSpeed)
	local scale = math.clamp(walkSpeed / maxWalkSpeed, 0, 1)
	MainScreenGui.WalkSpeedValueFrame.Size = UDim2.new(0, walkSpeedFullWidth * scale, 0, walkSpeedHeight)
end

local function updateJumpPowerFrame(jumpPower)
	local scale = math.clamp(jumpPower / maxJumpPower, 0, 1)
	MainScreenGui.JumpPowerValueFrame.Size = UDim2.new(0, jumpPowerFullWidth * scale, 0, jumpPowerHeight)
end

-- Initialize the frames with starting values
updateWalkSpeedFrame(currentWalkSpeed)
updateJumpPowerFrame(currentJumpPower)
applyWalkSpeed(currentWalkSpeed)
applyJumpPower(currentJumpPower)

-- WalkSpeed Slider Input
MainScreenGui.WalkSpeedSliderFrame.InputBegan:Connect(
	function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			local connection
			connection =
				input.Changed:Connect(
					function()
						if input.UserInputState == Enum.UserInputState.End then
							connection:Disconnect()
						end
					end
				)

			local mouse = game:GetService("Players").LocalPlayer:GetMouse()
			local xScale =
				math.clamp(
					(mouse.X - MainScreenGui.WalkSpeedSliderFrame.AbsolutePosition.X) / MainScreenGui.WalkSpeedSliderFrame.AbsoluteSize.X,
					0,
					1
				)
			currentWalkSpeed = math.floor(xScale * maxWalkSpeed)

			MainScreenGui.WalkSpeedValueText.Text = tostring(currentWalkSpeed)
			updateWalkSpeedFrame(currentWalkSpeed)
			applyWalkSpeed(currentWalkSpeed)
		end
	end
)

MainScreenGui.WalkSpeedSliderFrame.MouseMoved:Connect(
	function(x, y)
		if not game:GetService("UserInputService"):IsMouseButtonPressed(Enum.UserInputType.MouseButton1) then
			return
		end

		local xScale =
			math.clamp((x - MainScreenGui.WalkSpeedSliderFrame.AbsolutePosition.X) / MainScreenGui.WalkSpeedSliderFrame.AbsoluteSize.X, 0, 1)
		currentWalkSpeed = math.floor(xScale * maxWalkSpeed)

		MainScreenGui.WalkSpeedValueText.Text = tostring(currentWalkSpeed)
		updateWalkSpeedFrame(currentWalkSpeed)
		applyWalkSpeed(currentWalkSpeed)
	end
)

-- JumpPower Slider Input
MainScreenGui.JumpPowerSliderFrame.InputBegan:Connect(
	function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			local connection
			connection =
				input.Changed:Connect(
					function()
						if input.UserInputState == Enum.UserInputState.End then
							connection:Disconnect()
						end
					end
				)

			local mouse = game:GetService("Players").LocalPlayer:GetMouse()
			local xScale =
				math.clamp(
					(mouse.X - MainScreenGui.JumpPowerSliderFrame.AbsolutePosition.X) / MainScreenGui.JumpPowerSliderFrame.AbsoluteSize.X,
					0,
					1
				)
			currentJumpPower = math.floor(xScale * maxJumpPower)

			MainScreenGui.JumpPowerValueText.Text = tostring(currentJumpPower)
			updateJumpPowerFrame(currentJumpPower)
			applyJumpPower(currentJumpPower)
		end
	end
)

MainScreenGui.JumpPowerSliderFrame.MouseMoved:Connect(
	function(x, y)
		if not game:GetService("UserInputService"):IsMouseButtonPressed(Enum.UserInputType.MouseButton1) then
			return
		end

		local xScale =
			math.clamp((x - MainScreenGui.JumpPowerSliderFrame.AbsolutePosition.X) / MainScreenGui.JumpPowerSliderFrame.AbsoluteSize.X, 0, 1)
		currentJumpPower = math.floor(xScale * maxJumpPower)

		MainScreenGui.JumpPowerValueText.Text = tostring(currentJumpPower)
		updateJumpPowerFrame(currentJumpPower)
		applyJumpPower(currentJumpPower)
	end
)

-- Update character on respawn
game.Players.LocalPlayer.CharacterAdded:Connect(
	function(character)
		character:WaitForChild("Humanoid")
		applyWalkSpeed(currentWalkSpeed)
		applyJumpPower(currentJumpPower)
		updateWalkSpeedFrame(currentWalkSpeed)
		updateJumpPowerFrame(currentJumpPower)
	end
)

local noclipActive = false
local noclipConnection = nil
local player = game.Players.LocalPlayer

local function tweenFalseToggle(targetPos, color)
	if MainScreenGui.NoClipFalseToggle then
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local goal = {
			Position = UDim2.new(targetPos[1], 0, targetPos[2], 0),
			BackgroundColor3 = Color3.fromRGB(color[1], color[2], color[3])
		}
		local tween = TweenService:Create(MainScreenGui.NoClipFalseToggle, tweenInfo, goal)
		tween:Play()
	end
end

local function startNoclip()
	local character = player.Character
	if not character then return end

	noclipConnection = game:GetService("RunService").Stepped:Connect(function()
		for _, part in pairs(character:GetDescendants()) do
			if part:IsA("BasePart") and part.CanCollide then
				part.CanCollide = false
			end
		end
	end)
end

local function stopNoclip()
	if noclipConnection then
		noclipConnection:Disconnect()
		noclipConnection = nil
	end
end

local function enableNoclip()
	noclipActive = true
	startNoclip()
	tweenFalseToggle({0.5, 0.143}, {12, 118, 0})
end

local function disableNoclip()
	noclipActive = false
	stopNoclip()

	local character = player.Character
	if character then
		for _, part in pairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = true
			end
		end
	end

	tweenFalseToggle({0.054, 0.143}, {118, 0, 0})
end

-- Re-enable noclip if you respawn while it's active
player.CharacterAdded:Connect(function()
	if noclipActive then
		startNoclip()
	end
end)

-- Toggle noclip on button press
MainScreenGui.NoClipButton.MouseButton1Down:Connect(function()
	if noclipActive then
		disableNoclip()
	else
		enableNoclip()
	end
end)


local InfiniteJumpEnabled = false
local JumpConnection = nil

-- Tween function for UI.InfiniteJumpButton
local function tweenInfiniteJumpFalseToggle(targetPos, color)
	if MainScreenGui.InfiniteJumpFalseToggle then
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local goal = {
			Position = UDim2.new(targetPos[1], 0, targetPos[2], 0),
			BackgroundColor3 = Color3.fromRGB(color[1], color[2], color[3])
		}
		local tween = TweenService:Create(MainScreenGui.InfiniteJumpFalseToggle, tweenInfo, goal)
		tween:Play()
	end
end

-- Enable infinite jump
local function enableInfiniteJump()
	InfiniteJumpEnabled = true
	JumpConnection =
		UserInputService.JumpRequest:Connect(
			function()
				local humanoid =
				Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
				if humanoid then
					humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
				end
			end
		)
	tweenInfiniteJumpFalseToggle({0.5, 0.143}, {12, 118, 0}) -- Green and position 0.5
end

local function disableInfiniteJump()
	InfiniteJumpEnabled = false
	if JumpConnection then
		JumpConnection:Disconnect()
		JumpConnection = nil
	end
	tweenInfiniteJumpFalseToggle({0.054, 0.143}, {118, 0, 0})
end

-- Toggle infinite jump on UI.InfiniteJumpButton click
MainScreenGui.InfiniteJumpButton.MouseButton1Down:Connect(
	function()
		if InfiniteJumpEnabled then
			disableInfiniteJump()
		else
			enableInfiniteJump()
		end
	end
)

local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Tween helper
local function tweenFalseToggle(toggleObject, targetPos, color)
	if toggleObject then
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local goal = {
			Position = UDim2.new(targetPos[1], 0, targetPos[2], 0),
			BackgroundColor3 = Color3.fromRGB(color[1], color[2], color[3])
		}
		TweenService:Create(toggleObject, tweenInfo, goal):Play()
	end
end

local function RemoveHighlights()
	
end

local function RemoveNameTags()
	
end


MainScreenGui.HighlightAllButton.MouseButton1Down:Connect(function()
	RunService.RenderStepped:connect(function()
		roles = ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
		for i, v in pairs(roles) do
			if v.Role == "Murderer" then
				Murder = i
			elseif v.Role == 'Sheriff'then
				Sheriff = i
			elseif v.Role == 'Hero'then
				Hero = i
			end 
		end 
		CreateHighlight() 
		UpdateHighlights() 
	end) 
end) 

